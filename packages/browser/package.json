{
  "name": "debug-bridge-browser",
  "version": "0.2.1",
  "description": "Browser SDK for debug-bridge - enables AI agents to inspect and control web apps",
  "author": "stevengonsalvez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevengonsalvez/agent-bridge"
  },
  "keywords": ["debug", "bridge", "ai", "agent", "browser", "sdk", "websocket"],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "debug-bridge-types": "workspace:*",
    "html2canvas-pro": "^1.6.3"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
